<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListPreference
        android:title="@string/pref_artist_label"
        android:key="@string/pref_number_key"
        android:defaultValue="@string/pref_number_ten"
        android:entryValues="@array/pref_number_values"
        android:entries="@array/pref_number_options" />

    <ListPreference
        android:title="@string/pref_country_label"
        android:key="@string/pref_country_key"
        android:defaultValue="@string/pref_country_jp"
        android:entryValues="@array/pref_country_values"
        android:entries="@array/pref_country_options" />

</PreferenceScreen>
